{
  "version": 2,
  "builds": [
    {
      "src": "dist-server/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/dist-server/index.js"
    }
  ]
}
